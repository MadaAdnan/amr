{
  "name": "lavish/ride",
  "type": "project",
  "description": "Luxury Car Services.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "anhskohbo/no-captcha": "^3.5",
    "barryvdh/laravel-dompdf": "^0.9.0",
    "doctrine/dbal": "^3.1",
    "fideloper/proxy": "^4.4",
    "firebase/php-jwt": "^6.9",
    "fruitcake/laravel-cors": "^2.0",
    "giggsey/libphonenumber-for-php": "^8.13",
    "google/apiclient": "^2.15",
    "guzzlehttp/guzzle": "^7.5",
    "laravel/fortify": "^1.19",
    "laravel/framework": "^8.12",
    "laravel/passport": "^10.2",
    "laravel/socialite": "^5.10",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.2",
    "maatwebsite/excel": "^3.1",
    "phpoffice/phpspreadsheet": "^1.29",
    "realrashid/sweet-alert": "^7.0",
    "santigarcor/laratrust": "^6.3",
    "socialiteproviders/apple": "^5.6",
    "spatie/crawler": "^7.1",
    "spatie/laravel-activitylog": "^4.7",
    "spatie/laravel-analytics": "4.0.1",
    "spatie/laravel-medialibrary": "^9.12",
    "spatie/laravel-permission": "^5.8",
    "spatie/laravel-sitemap": "^6.3",
    "stevebauman/location": "^6.2.0",
    "stripe/stripe-php": "^7.77",
    "torann/geoip": "^3.0",
    "twilio/sdk": "^6.21"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "files": [
         ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
